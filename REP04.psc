//Mod = Si es divisible por tal numero
Algoritmo REP04
	Definir numero Como Entero
	numero <- 1
	
	Repetir
		Si numero mod 5 = 0 Entonces
			Escribir numero
		FinSi
		numero <- numero + 1
	Hasta Que numero > 100
	
FinAlgoritmo
